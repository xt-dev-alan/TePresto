<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

        <record id="seq_pawn_partner" model="ir.sequence">
            <field name="name">Partners</field>
            <field name="code">pawn.partner</field>
            <field name="prefix">PPA</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    
        <record id="seq_pawn_pawn" model="ir.sequence">
            <field name="name">Pawns</field>
            <field name="code">pawn.pawn</field>
            <field name="prefix">PA</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_pawn_order" model="ir.sequence">
            <field name="name">Pawn Orders</field>
            <field name="code">pawn.order</field>
            <field name="prefix">PAO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    
    </data>

</odoo>
